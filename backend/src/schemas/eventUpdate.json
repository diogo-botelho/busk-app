{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "type": {
      "type": "string"
    },
    "coordinates": {
      "type": "object",
      "properties": {
        "lat": {"type" : "number"},
        "lng": {"type" : "number"}
      }
    },
    "date": {
      "type": "string"
    },
    "startTime": {
      "type": "string"
    },
    "endTime": {
      "type": "string"
    }
  },
  "additionalProperties": false
}